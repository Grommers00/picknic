{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "align": [
            true,
            "statements",
            "members",
            "elements"
        ],
        "max-line-length": [
            true,
            {
                "limit": 120,
                "ignore-pattern": "^\\s*import |^\\s*export |^\\s*const .+? = \".+\";|\\s*\".*?\","
            }
        ],
        "no-console": false
    },
    "rulesDirectory": []
}